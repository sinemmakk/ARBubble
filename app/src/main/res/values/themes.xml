        <item name="colorPrimary">#F38181</item>
        <item name="colorPrimaryVariant">#292E6C</item>
        <item name="colorOnPrimary">#292E6C</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
